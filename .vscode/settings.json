{
  "files.associations": {
    "*.lss": "asm",
    "cstddef": "cpp",
    "tuple": "cpp"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "_site": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "_site": true
  },
  "python.pythonPath": "/usr/bin/python",
}